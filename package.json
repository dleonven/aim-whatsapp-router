{
	"name": "aim-whatsapp-router",
	"version": "1.0.0",
	"description": "WhatsApp Router for AIM Global - Routes leads from main WhatsApp to agent numbers",
	"main": "src/index.js",
	"scripts": {
		"start": "node src/index.js",
		"dev": "node --watch src/index.js",
		"prod": "pm2 start ecosystem.config.js --env production",
		"stop": "pm2 stop aim-whatsapp-router",
		"restart": "pm2 restart aim-whatsapp-router",
		"logs": "pm2 logs aim-whatsapp-router"
	},
	"dependencies": {
		"express": "^4.18.2",
		"better-sqlite3": "^9.4.3",
		"dotenv": "^16.4.1",
		"axios": "^1.6.7"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
